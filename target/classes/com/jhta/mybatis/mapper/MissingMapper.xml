<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jhta.mybatis.mapper.missing">
	<select id="getinfo" parameterType="int" resultType="missing">
		select * from missing where missNum=#{missNum}
	</select>
	<select id="count" parameterType="hashmap">
		select NVL(count(*),0) from missing
		<where>
			<if test=""></if>
			<if test=""></if>
		</where>
	</select>
</mapper>